{
	"name": "DarkAlchemy",
	"colors": {
		"foreground": "#e5e5e5",
		"focusBorder": "#AD9CFF",
		"selection.background": "#6f6f6f",
		"scrollbar.shadow": "#0000007e",
		"input.background": "#0F0E0E",
		"input.foreground": "#999",
		"button.background": "#edffa5",
		"button.foreground": "#151515",
		"button.hoverBackground": "#dcff50",
		"textLink.foreground": "#e5e5e5",
		"button.secondaryForeground": "#c5c5c5",
		"button.secondaryHoverBackground": "#373737",

		"activityBar.foreground": "#dedede",
		"activityBar.background": "#000",
		"activityBar.inactiveForeground": "#999",
		"activityBarBadge.foreground": "#000",
		"activityBarBadge.background": "#edffa5",
		"activityBar.border": "#000000",
		// "actibityBar.activeBackground": "#151515",

		"sideBar.background": "#000",
		"sideBar.foreground": "#999",
		"sideBarSectionHeader.background": "#151515",
		"sideBarSectionHeader.border": "#00000000",
		"sideBarSectionHeader.foreground": "#e5e5e5",
		"sideBarTitle.foreground": "#e5e5e5",
		"sideBar.border": "#00000000",
		"list.dropBackground": "#151515",
		"list.focusForeground": "#808080",
		"list.focusBackground": "#e5e5e51a",
		"list.highlightForeground": "#e5e5e5",

		"statusBar.background": "#000",
		"statusBar.foreground": "#808080",
		"statusBar.border": "#00000000",
		"statusBar.debuggingBackground": "#563300",
		"statusBar.debuggingForeground": "#999",
		"statusBar.noFolderBackground": "#5d3fe0",
		"statusBar.noFolderForeground": "#e5e5e5",
		"statusBarItem.remoteBackground": "#cef144",
		"statusBarItem.remoteForeground": "#151515",

		"tab.activeBackground": "#151515",
		"tab.activeForeground": "#e5e5e5",
		"tab.inactiveBackground": "#000",
		"tab.inactiveForeground": "#999",
		"tab.hoverBackground": "#E5E5E51A",
		"tab.border": "#151515",

		"titleBar.activeBackground": "#000",
		"titleBar.activeForeground": "#808080",
		"titleBar.border": "#00000000",
		"titleBar.inactiveBackground": "#000",

		"menu.foreground": "#e5e5e5",
		"menu.background": "#373737",
		"menubar.selectionForeground": "#c5c5c5",
		"menubar.selectionBackground": "#e5e5e51a",
		"menu.selectionForeground": "#e5e5e5",
		"menu.selectionBackground": "#e5e5e51a",
		"menu.selectionBorder": "#00000000",
		"menu.border": "#00000000",

		"editor.background": "#151515",
		"editorLineNumber.foreground": "#858585",
		"editorLineNumber.activeForeground": "#c6c6c6",

		"breadcrumb.background": "#151515",
		"breadcrumb.foreground": "#999",
		"breadcrumb.focusForeground": "#e5e5e5",
		"editorGroupHeader.border": "#00000000",
		"editorGroupHeader.tabsBackground": "#000",

		"scrollbarSlider.background": "#79797966",
		"scrollbarSlider.hoverBackground": "#646464b3",
		"scrollbarSlider.activeBackground": "#bfbfbf66",

		"widget.shadow": "#0000005c",
		"editorWidget.foreground": "#ccc",
		"editorWidget.background": "#252526",
		"pickerGroup.border": "#3f3f46",
		"pickerGroup.foreground": "#3794ff",
		"editorWidget.resizeBorder": "#5f5f5f",
		"debugToolBar.background": "#333",
		"debugToolBar.border": "#474747",

		"errorForeground": "#f48771",
		// "warningForeground": "#F7CC66",
		"editorError.foreground": "#f48771",
		"list.errorForeground": "#f48771",
		"list.warningForeground": "#F7CC66",
		"editorWarning.foreground": "#F7CC66",

		"gitDecoration.deletedResourceForeground": "#C54444",
		"gitDecoration.untrackedResourceForeground": "#9FE7A0",
		"gitDecoration.conflictingResourceForeground": "#ED6C6C",
		"gitDecoration.ignoredResourceForeground": "#585858",
		"gitDecoration.modifiedResourceForeground": "#DD763C"
	},
	"tokenColors": [
		{
      "name": "Strong",
      "scope": "strong, markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasis",
      "scope": "emphasis, markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment, string.quoted.docstring",
      "settings": {
        "foreground": "#616263",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Type",
      "scope": "storage.type.primitive, support.type, entity.name.type, keyword.type, keyword.other.unit",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "String",
      "scope": "string, meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, support.constant",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Undefined",
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.require, variable.language.super, keyword.other.get, keyword.other.set",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Constants",
      "scope": "variable.other.constant.property",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class entity.name.type.module",
      "settings": {
        "foreground": "#ff5c57",
		"fontStyle": "italic"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
	{
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, storage.modifier, support.type.object, constant.language, variable.language.this, variable.language.special.self",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
	{
      "name": "Storage Modifier [e.g. class, extends, static] (JS",
      "scope": "storage.modifier, storage.type.class.js, variable.language.this, variable.language.this.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Diff insertion",
      "scope": "markup.inserted.diff, meta.diff.header.to-file",
      "settings": {
        "foreground": "#5af78e",
        "background": "#616263"
      }
    },
    {
      "name": "Diff deletion",
      "scope": "markup.deleted.diff, meta.diff.header.from-file",
      "settings": {
        "foreground": "#ff5c57",
        "background": "#616263"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "C# method call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#616263"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "CSS property",
      "scope": "source.css support.type.property-name",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f3f99d",
		"fontStyle": "italic"
      }
    },
    {
      "name": "HTML tag expression",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Java package & import",
      "scope": "source.java storage.modifier.package, source.java storage.modifier.import",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Java annotation",
      "scope": "source.java storage.type.annotation",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Java method call",
      "scope": "source.java meta.method-call meta.method",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Java inner-class variable",
      "scope": "source.java meta.method-call meta.inner-class meta.method",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Java inner-class method call",
      "scope": "source.java meta.method-call meta.inner-class meta.method meta.method-call meta.method",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Java inner-class method",
      "scope": "source.java meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Java null",
      "scope": "source.java constant.language",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "Async",
      "scope": "source.js storage.modifier.async",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#57c7ff"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference, markup.underline.link.image",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Markdown block quote",
      "scope": "text.html.markdown markup.quote",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#f3f99d"
      }
    },
    {
      "name": "Markdown code",
      "scope": "text.html.markdown markup.inline.raw",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Python import *",
      "scope": "source.python keyword.operator.arithmetic",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Python def",
      "scope": "source.python meta.function storage.type.function",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Python function arguments",
      "scope": "source.python meta.function-call.arguments",
      "settings": {
        "foreground": "#eff0eb"
      }
    },
    {
      "name": "Python function decorators",
      "scope": "source.python meta.function entity.name.function.decorator",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Python True & False & None",
      "scope": "source.python constant.language",
      "settings": {
        "foreground": "#5af78e"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#ff5c57"
      }
    }
	]
}
